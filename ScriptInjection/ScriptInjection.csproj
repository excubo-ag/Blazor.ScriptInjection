<Project Sdk="Microsoft.NET.Sdk.Razor">
  <Import Project="package.csproj" />
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RazorLangVersion>3.0</RazorLangVersion>
    <AssemblyName>Excubo.Blazor.ScriptInjection</AssemblyName>
    <RootNamespace>Excubo.Blazor.ScriptInjection</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Excubo.Analyzers.Annotations" Version="1.0.1" />
    <PackageReference Include="Excubo.Analyzers.DependencyInjectionValidation" Version="1.0.4">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="3.1.4" />
  </ItemGroup>

</Project>
